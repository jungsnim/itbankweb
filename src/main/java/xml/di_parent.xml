<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
 
	<bean name="tire" class="model.car.Tire" 
	p:name="한국타이어" p:size="16"/>
	
	<bean name="engine" class="model.car.Engine" 
	 p:hp="200" p:torque="70" p:name="12기통"/>
	 
	 <bean name="smartKey" class="model.car.SmartKey" 
	 p:channel="13"/>
	 
	 <bean name="sunroof" class="model.car.SunRoof" 
	 p:color="레드" p:size="30"/>

</beans>